        <Label Content="Can" HorizontalAlignment="Left" Margin="20,48,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <Rectangle Fill="{DynamicResource ZuneColor2Brush}" HorizontalAlignment="Left" Height="100" Margin="6,81,0,0" Stroke="Black" VerticalAlignment="Top" Width="94"/>
        <CheckBox Content="     R1" x:Name="R1" HorizontalAlignment="Left" Margin="25,98,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox Content="     Y1" x:Name="Y1" HorizontalAlignment="Left" Margin="25,124,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox Content="     G1" x:Name="G1" HorizontalAlignment="Left" Margin="25,150,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <Rectangle Fill="{DynamicResource ZuneColor2Brush}" HorizontalAlignment="Left" Height="100" Margin="6,184,0,0" Stroke="Black" VerticalAlignment="Top" Width="94"/>
        <CheckBox x:Name="R2" Content="     R2" HorizontalAlignment="Left" Margin="25,201,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox x:Name="Y2" Content="     Y2" HorizontalAlignment="Left" Margin="25,227,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox x:Name="G2" Content="     G2" HorizontalAlignment="Left" Margin="25,253,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <Rectangle Fill="{DynamicResource ZuneColor2Brush}" HorizontalAlignment="Left" Height="100" Margin="6,287,0,0" Stroke="Black" VerticalAlignment="Top" Width="94"/>
        <CheckBox x:Name="R3" Content="     R3" HorizontalAlignment="Left" Margin="25,304,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox x:Name="Y3" Content="     Y3" HorizontalAlignment="Left" Margin="25,330,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox x:Name="G3" Content="     G3" HorizontalAlignment="Left" Margin="25,356,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <Rectangle Fill="{DynamicResource ZuneColor2Brush}" HorizontalAlignment="Left" Height="100" Margin="6,389,0,0" Stroke="Black" VerticalAlignment="Top" Width="94"/>
        <CheckBox x:Name="R4" Content="     R4" HorizontalAlignment="Left" Margin="25,406,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox x:Name="Y4" Content="     Y4" HorizontalAlignment="Left" Margin="25,432,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox x:Name="G4" Content="     G4" HorizontalAlignment="Left" Margin="25,458,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FlowDirection="RightToLeft" FontSize="16" FontWeight="Bold"/>
        <CheckBox Content="{DynamicResource tsc_lightcheck_all}" HorizontalAlignment="Left" Margin="25,519,0,0" VerticalAlignment="Top" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked"/>
    </Grid>
</UserControl>
